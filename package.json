{
  "name": "keyboard-barcode-scan-listener",
  "version": "1.0.3",
  "description": "Listen for keyboard sequences and interpret them as a barcode.",
  "main": "lib/index.js",
  "files": [
    "lib/**"
  ],
  "repository": "git@github.com:goodeggs/keyboard-barcode-scan-listener.git",
  "author": "Good Eggs <open-source@goodeggs.com>",
  "license": "MIT",
  "scripts": {
    "build": "yarn run clean && babel --extensions .js,.jsx,.ts,.tsx --source-maps -d lib src && yarn run generate:types",
    "clean": "rm -rf lib",
    "fmt": "yarn run lint --fix",
    "fmt:glob": "yarn run lint:glob --fix",
    "generate:types": "tsc --noEmit false --declaration --emitDeclarationOnly",
    "lint": "yarn run lint:glob .",
    "lint:glob": "eslint --ext js,jsx,ts,tsx --ignore-path .gitignore --ignore-pattern '!.*rc.js'",
    "prepublishOnly": "yarn build",
    "test": "yarn run todos && yarn run lint && yarn run typecheck && yarn run test:unit",
    "test:unit": "jest $JEST_FLAGS",
    "test:unit:watch": "yarn run test:unit --watch",
    "todos": "leasot '**/*.{js,jsx,ts,tsx}' --exit-nicely --tags '@ts-ignore' --ignore='node_modules/**','build/**','flow-typed/**','.git/**'",
    "typecheck": "tsc"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/cli": "7.4.4",
    "@babel/core": "7.2.2",
    "@babel/preset-env": "7.2.3",
    "@babel/preset-typescript": "7.1.0",
    "@babel/register": "7.4.4",
    "@goodeggs/tsconfig": "^1.0.0",
    "@types/jest": "^23.3.10",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-goodeggs": "^7.2.0",
    "eslint-plugin-jest": "^22.1.2",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^3.0.4",
    "jest": "^23.6.0",
    "leasot": "^7.2.0",
    "lint-staged": "^8.1.0",
    "prettier": "1.17.1",
    "typescript": "^3.2.2",
    "typescript-eslint-parser": "^22.0.0"
  }
}
